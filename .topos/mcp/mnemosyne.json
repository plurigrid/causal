{
  "mcpServers": {
    "mnemosyne": {
      "command": "uv",
      "args": ["run", "neem-mcp-server"],
      "env": {
        "MNEMOSYNE_FASTAPI_URL": "http://127.0.0.1:8001",
        "LOG_LEVEL": "ERROR"
      },
      "description": "sophia-labs/mnemosyne-mcp â€” RDF knowledge graph for proof transcripts",
      "source": "https://github.com/sophia-labs/mnemosyne-mcp",
